<html>
<head>
    <title>Resumes</title>
</head>
<body>
<h1>Resumes</h1>
<table border="1">
    <tr><th>First Name</th><th>Last Name</th><th>Resume Name</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/account/?first_name=<%= result[i].first_name %>"><%= result[i].first_name %></a></td>
        <td><a href="/account/?last_name=<%= result[i].last_name %>"><%= result[i].last_name %></a></td>
        <td><a href="/resume/?resume_name=<%= result[i].resume_name %>"><%= result[i].resume_name %></a></td>
    </tr>
    <% } %>
</table>
</body>

</html>